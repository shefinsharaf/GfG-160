class Solution {
  public:
    // Function to find hIndex
    int hIndex(vector<int>& citations) {
        // code here
        sort(citations.begin(),citations.end(),greater<int>());
        int h = 0;
        for(int i =0;i<citations.size();i++)
        {
            if(citations[i] >= i+1)
            h = i+1;
            else 
            break;
        }
        return h;
    }
};
